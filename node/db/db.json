    {
      "id": 1,
      "title": "Pasta Alfredo",
      "description": "Pasta with a rich cream sauce",
      "authorId": 1,
      "ingredients": [
        "200g pasta",
        "100ml cream",
        "50g parmesan cheese",
        "1 clove garlic",
        "Salt",
        "Pepper"
      ],
      "instructions": "Cook the pasta according to the package instructions. In a separate pan, heat the cream and add the garlic. Stir in the parmesan cheese until melted. Combine the pasta with the sauce and season with salt and pepper."
    },
    {
      "id": 2,
      "title": "Chicken Curry",
      "description": "Spicy and flavorful chicken curry",
      "authorId": 1,
      "ingredients": [
        "500g chicken breast",
        "1 onion",
        "2 cloves garlic",
        "400ml coconut milk",
        "Salt",
        "Pepper"
      ],
      "instructions": "Heat the oil in a pan and sauté the chopped onion and garlic until golden. Add the chicken pieces and cook until browned. Stir in the curry powder and cook for another minute. Pour in the coconut milk and simmer for 20 minutes. Season with salt and pepper."
    },
    {
      "id": 3,
      "title": "Chocolate Cake",
      "description": "Rich and moist chocolate cake",
      "authorId": 1735048531744,
      "ingredients": [
        "200g flour",
        "200g sugar",
        "100g cocoa powder",
        "1 tsp baking powder",
        "1 tsp baking soda",
        "2 eggs",
        "240ml milk",
        "120ml vegetable oil",
        "2 tsp vanilla extract",
        "240ml boiling water"
      ],
      "instructions": "Preheat the oven to 175°C (350°F). In a large bowl, mix the flour, sugar, cocoa powder, baking powder, and baking soda. Add the eggs, milk, oil, and vanilla extract, and mix until smooth. Gradually add the boiling water and mix until well combined. Pour the batter into a greased baking pan and bake for 30-35 minutes. Let cool before serving."
    },
